#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules/vitepress/bin/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules/vitepress/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules/vitepress/bin/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules/vitepress/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/vitepress@1.0.0-alpha.28_@a_63dbfe338c84b4769c7969ed6bb53186/node_modules:/mnt/d/works/Project/AFulcrum.github.io/fulcrum/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
